<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="ct.common.hibernate.example2.Parameter" table="PUBLIC.PARAMETER">
		<id name="parameterId" column="PARAMETERID" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="java.lang.String" update="true" insert="true" access="property" column="NAME" />
		<property name="type" type="java.lang.String" update="true" insert="true" access="property" column="TYPE" />
		<property name="value" type="java.lang.String" update="true" insert="true" access="property" column="VALUE" />
	</class>
</hibernate-mapping>         